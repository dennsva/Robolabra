package robotti;

import lejos.nxt.*;

/**
 * Example leJOS Project with an ant build file
 *
 */
public class Robotti {

	public static void main(String[] args) {
		//alas = A forward
		Motor.A.setSpeed(180);
		Motor.A.forward();
		
		while (true) {
			Button.waitForPress();
			Motor.A.rotate(360);
		}
		
		//eteenp√§in = B forward
		//Motor.B.setSpeed(180);
		//Motor.B.forward();
		//Motor.B.rotate(720);
	}
}
